output "LG Display 0x0521 Unknown" alias $t480 position 0,0
output "California Institute of Technology 0x1626 0x00006002" alias $ll7 position 0,0 scale 1.5
output "Dell Inc. DELL S2722QC 192SH24" alias $ext scale 1.5 adaptive_sync off

profile ll7-home {
  output $ll7 scale 2
  output $ext position 1600,0
}

profile ll7 {
  output $ll7 scale 1.5
}

profile t480-home {
  output $t480
  output $ext position 0,-2000
}

profile t480 {
  output $t480
}
